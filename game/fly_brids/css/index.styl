#wrapbg
    width 343px
    height 480px
    margin 0 auto
    background-image: url(../img/bg.jpg)
    position relative
    top 100px
    user-select none
    overflow hidden

.headTitle
    width 236px
    height 77px
    background-image: url(../img/head.jpg)
    position absolute
    left 53px
    top 100px
    // animation: updown 2s linear infinite

@keyframes updown
    0%,100%
        transform translateY(0)
    50%
        transform translateY(10px)


#headBird
    position absolute
    right 0
    margin-top 25px

#startBtn
    width 85px
    height 29px
    padding 0
    margin 0
    background url(../img/start.jpg)
    position absolute
    left 129px
    top 250px

#grassLand1
    height 14px
    width 343px
    background-image url(../img/banner.jpg)
    position absolute
    top 423px
    left 0
    // animation: scroll 2s linear infinite


#grassLand2
    height 14px
    width 343px
    background-image url(../img/banner.jpg)
    position absolute
    top 423px
    left 343px
    // animation: scroll 2s linear infinite

@keyframes scroll
    0%
        transform translateX(0)
    100%
        transform translateX(-343px)

